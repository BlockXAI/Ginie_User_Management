{
  "name": "evi-user-api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node --experimental-specifier-resolution=node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint .",
    "prisma:generate": "prisma generate",
    "prisma:pull": "prisma db pull",
    "prisma:migrate:dev": "prisma migrate dev --name init_phase0",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "smoke": "bash scripts/smoke.sh",
    "test:phase1:smoke": "bash scripts/phase1_smoke.sh",
    "test:phase1:negative": "bash scripts/phase1_negative.sh"
  },
  "dependencies": {
    "@prisma/client": "^5.15.1",
    "@sentry/node": "^10.21.0",
    "argon2": "^0.40.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "ioredis": "^5.4.1",
    "pg": "^8.11.5",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.20",
    "@types/pg": "^8.11.6",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/ws": "^8.5.12",
    "eslint": "^8.57.0",
    "prisma": "^5.15.1",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
